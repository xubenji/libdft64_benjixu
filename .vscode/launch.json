{
    "configurations": [
        
        
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
           "program": "/root/libdft64/tools/obj-intel64/track.so",
            "cwd": "${workspaceFolder}/tools/",
            "processId": "18378",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}